{
    "version": 2,
    "builds": [
      {
        "src": "api/main.py",
        "use": "@vercel/python"
      },
      {
        "src": "static/index.html",
        "use": "@vercel/static"
      },
    {
        "src": "static/style.css",
        "use": "@vercel/css"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/main.py"
      },
      {
        "src": "/(.*)",
        "dest": "static/index.html"
      }
    ]
  }
  
