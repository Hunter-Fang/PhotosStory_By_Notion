{
  "version": 2,
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/index.html",
      "use": "@vercel/static",
      "headers": [
        {
          "source": "**/*.css",
          "headers": [
            {
              "key": "Content-Type",
              "value": "text/css"
            }
          ]
        }
      ]
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "static/index.html"
    }
  ]
}
